<?xml version="1.0" encoding="UTF8"?>
<messages>

    <message id="render-msg">
        <div class="container3-col">
            <div class="button-box2 containeritem-navbar">
                <div class="float-left">
                    ${code}
                    ${memory}
                    <div class="button-box-item2-2-box">
                        ${console}
                        <div class="popup-div-console" id="console-popup">Need Input</div>
                    </div>
                    ${clear-btn}
                </div>
                <div class="float-right">
                    <div class="popup-div-download">
                        ${download}
                    </div>
                    <div class="popup-div-assemble">
                        ${compile}
                    </div>
                    <div class="popup-div-run">
                        ${run}
                    </div>
                    <div class="popup-div-debug">
                        ${debug}
                    </div>
                </div>
            </div>
            <div class="button-box3 containeritem-navbar2">
                <div class="float-left2">
                    <div class="popup-div-next">
                        ${next}
                    </div>
                    <div class="popup-div-continue">
                        ${continue}
                    </div>
                    <div class="popup-div-stop">
                        ${stop}
                    </div>
                </div>
                <div class="float-right">
                    <div class="popup-div-clr">
                        ${clr}
                    </div>
                </div>
            </div>
            <div class="containeritem-widget">
                <div>
                    ${widget}
                    <div class="popup-div-editor" id="editor-error-label"></div>
                </div>
            </div>

            <div class="containeritem-widget2">
                ${register_render}
            </div>
        </div>
    </message>

</messages>